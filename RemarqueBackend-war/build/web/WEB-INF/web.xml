<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>source.Signup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CredentialVerifier</servlet-name>
        <servlet-class>source.CredentialVerifier</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NoteOperations</servlet-name>
        <servlet-class>source.NoteOperations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadNotes</servlet-name>
        <servlet-class>source.LoadNotes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetFeedback</servlet-name>
        <servlet-class>source.GetFeedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CollaboratorOperations</servlet-name>
        <servlet-class>source.CollaboratorOperations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>source.logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/Signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CredentialVerifier</servlet-name>
        <url-pattern>/CredentialVerifier</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NoteOperations</servlet-name>
        <url-pattern>/NoteOperations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadNotes</servlet-name>
        <url-pattern>/LoadNotes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetFeedback</servlet-name>
        <url-pattern>/GetFeedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CollaboratorOperations</servlet-name>
        <url-pattern>/CollaboratorOperations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
